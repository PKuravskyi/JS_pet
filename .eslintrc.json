{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": "eslint:recommended",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"indent": ["error", "tab", { "SwitchCase": 1 }],
		"quotes": ["error", "single", { "avoidEscape": true }],
		"semi": ["error", "always"],
		"no-undef": "off",
		"linebreak-style": "off"
	}
}
